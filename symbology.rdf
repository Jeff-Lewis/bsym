  xmlns:skos="http://www.w3.org/2004/02/skos/core#"
  xmlns:figi-gii="http://www.omg.org/spec/FIGI/GlobalInstrumentIdentifiers/"
  xmlns:gas="http://schema.ga-group.nl/symbology#">

