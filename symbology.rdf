    <dct:license rdf:datatype="&xsd;anyURI">http://opensource.org/licenses/mit-license.php</dct:license>

    <owl:versionIRI rdf:resource="http://schema.ga-group.nl/0.1/symbology#"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
  </owl:Ontology>

  <!--
  //////////////////////////////////////////////////////////////////////
  //
  // Object Properties
  //
  //////////////////////////////////////////////////////////////////////
  -->

  <owl:ObjectProperty rdf:about="&gas;sector">
    <rdfs:label>sector</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">maps a financial instrument identifier to a market sector</skos:definition>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="&gas;listedOn">
    <rdfs:label>listed on</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">relates a financial instrument to an exchange, a pricing source, or a data provider</skos:definition>
    <rdfs:domain rdf:resource="&figi-gii;FinancialInstrumentIdentifier"/>
    <rdfs:range rdf:resource="&figi-gii;Code"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="&gas;listedAs">
    <rdfs:label>listed as</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">relates a financial instrument (identified by its identifier) to a ticker symbol or another Identifier</skos:definition>
    <rdfs:domain rdf:resource="&figi-gii;FinancialInstrumentIdentifier"/>
    <rdfs:range rdf:resource="&figi-gii;Identifier"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="&gas;symbolOf">
    <rdfs:label>symbol of</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">relates an identifier to its data vendor</skos:definition>
    <rdfs:domain rdf:resource="&figi-gii;Identifier"/>
    <rdfs:range rdf:resource="&figi-gii;Symbology"/>
  </owl:ObjectProperty>

  <!--
  //////////////////////////////////////////////////////////////////////
  //
  // Datatype Properties
  //
  //////////////////////////////////////////////////////////////////////
  -->

  <owl:DatatypeProperty rdf:about="&gas;listedSince">
    <rdfs:label>listed since</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">indicates since when a financial instrument (identified by its identifier) has been listed</skos:definition>
    <rdfs:domain rdf:resource="&figi-gii;FinancialInstrumentIdentifier"/>
    <rdfs:range rdf:resource="&xsd;dateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="&gas;listedTill">
    <rdfs:label>listed till</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">indicates till when a financial instrument (identified by its identifier) had been listed</skos:definition>
    <rdfs:domain rdf:resource="&figi-gii;FinancialInstrumentIdentifier"/>
    <rdfs:range rdf:resource="&xsd;dateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="&gas;validSince">
    <rdfs:label>valid since</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">generic valid time</skos:definition>
    <rdfs:range rdf:resource="&xsd;dateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="&gas;validTill">
    <rdfs:label>valid till</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">generic valid time</skos:definition>
    <rdfs:range rdf:resource="&xsd;dateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="&gas;secu">
    <rdfs:label>ariva secu number</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">relates an identifier to ariva's secu number</skos:definition>
    <rdfs:domain rdf:resource="&figi-gii;ShareClassGlobalIdentifier"/>
    <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="&gas;conid">
    <rdfs:label>interactive brokers contract identifier</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">relates an identifier to IB's conid number</skos:definition>
    <rdfs:domain rdf:resource="&figi-gii;ShareClassGlobalIdentifier"/>
    <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
  </owl:DatatypeProperty>

  <!--
  //////////////////////////////////////////////////////////////////////
  //
  // Classes
  //
  //////////////////////////////////////////////////////////////////////
  -->

  <owl:Class rdf:about="&gas;DataVendor">
    <rdfs:label>data vendor</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&figi-gii;isIdentifiedBy"/>
        <owl:onClass rdf:resource="&figi-gii;Symbology"/>
        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <skos:definition>A financial data vendor provides data to financial firms, traders, and investors. The data distributed is collected from sources such as a stock exchange feeds, brokers and dealer desks or regulatory filings (e.g. an SEC filing)</skos:definition>
    <dct:source>http://en.wikipedia.org/wiki/Financial_data_vendor</dct:source>
  </owl:Class>

  <owl:Class rdf:about="&gas;Symbology">
    <rdfs:label>symbology</rdfs:label>
    <skos:definition>A collection of symbols</skos:definition>
  </owl:Class>

</rdf:RDF>
